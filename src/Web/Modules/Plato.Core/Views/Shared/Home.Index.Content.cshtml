
<caching vary-by-user="true" expires-after="@TimeSpan.FromMinutes(10)">
    <card class="card card-body-flush">
        <card-title>
            <h6 class="card-title">@T["Browse"]</h6>
        </card-title>
        <card-body>
            <div class="list-group list-group-flush list-group-hover home-page-tiles">
                <div class="row no-gutters">
                    <navigation name="home" enable-list="false" />
                </div>
            </div>
        </card-body>
    </card>
</caching>
